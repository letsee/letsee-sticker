{
  "parser": "babel-eslint",

  "extends": [
    "airbnb",
    "plugin:flowtype/recommended"
  ],

  "rules": {
    "react/jsx-filename-extension": "off",
    "indent": [0, 2],
    "no-trailing-spaces": [2, { "skipBlankLines": true }],
    "no-case-declarations": 0,
    "max-len": [2, 10000, 4],
    "no-tabs": 0,
    "quotes": 0,
    "react/jsx-indent": [0, 2],
    "react/jsx-tag-spacing": 0,
    "react/jsx-closing-bracket-location": 0,
    "jsx-a11y/no-noninteractive-element-interactions": 0,
    "jsx-a11y/alt-text": 0,
    "arrow-body-style": 0,
    "comma-dangle": 0,
    "no-mixed-operators": 0,
    "key-spacing": 0
  },

  "plugins": [
    "flowtype",
    "react",
    "jsx-a11y",
    "import"
  ],

  "env": {
    "browser": true
  },

  "globals": {
    "letsee": true,
    "DOMRenderable": true,
    "Vector3": true,
    "Quaternion": true,
    "Euler": true,
    "Object3D": true,
    "Kakao": true,
    "quotes": false
  }
}
